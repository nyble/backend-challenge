{
  "name": "take-home-challenge",
  "version": "1.0.0",
  "description": "Unicorn Corp. ðŸ¦„ Payment Routing Service",
  "main": "src/server.ts",
  "scripts": {
    "up": "yarn nodemon --legacy-watch --config nodemon.json",
    "build": "ttsc --project ./tsconfig.json",
    "style": "yarn eslint --fix src",
    "server": "node build/src/server.js",
    "server:dev": "ts-node --project ./tsconfig.json --compiler ttypescript src/server.ts",
    "server:debug": "export TS_NODE_PROJECT=./tsconfig.json && export TS_NODE_COMPILER=ttypescript && node -r ts-node/register --inspect=0.0.0.0:9873 src/server.ts",
    "test:all": "yarn run test:unit && yarn run test:integration",
    "test:integration": "STAGE=dev yarn ts-mocha --color --timeout 50000 --paths --p ./tsconfig.json test/integration/**/*.ts",
    "test:unit": "STAGE=dev yarn ts-mocha --color --timeout 20000 --paths --p ./tsconfig.json test/unit/**/*.ts",
    "clean": "rm -rf ./dist"
  },
  "pre-commit": [
    "style"
  ],
  "author": "",
  "license": "ISC",
